{
  "name": "@cloudsail/auto-deploy",
  "private": false,
  "version": "0.1.0",
  "description": "一个WEB前端自动化部署cli工具",
  "license": "MIT",
  "type": "module",
  "files": [
    "bin",
    "src",
    "deploy.config.js",
    "index.d.ts"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./index.d.ts",
  "exports": {
    "types": "./index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.umd.cjs"
  },
  "bin": {
    "autodeploy": "bin/auto-deploy.js"
  },
  "scripts": {
    "dev": "node bin/auto-deploy.js",
    "demo": "vite",
    "clear": "rimraf node_modules && rimraf package-lock.json && rimraf .eslintcache && rimraf pnpm-lock.yaml && rimraf yarn.lock",
    "reinstall": "npm run clear && yarn",
    "prepublish": "npm config set registry https://registry.npmjs.org",
    "postpublish": "npm config set registry https://registry.npm.taobao.org"
  },
  "dependencies": {
    "archiver": "5.3.1",
    "chalk": "4.1.2",
    "commander": "^10.0.1",
    "cosmiconfig": "^8.1.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "inquirer": "^9.0.0",
    "ora": "5.1.0",
    "ssh2": "^1.0.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.0",
    "vite": "^4.0.4"
  },
  "keywords": [
    "cli",
    "deploy",
    "automation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vCloudSail/auto-deploy"
  },
  "bugs": {
    "url": "https://github.com/vCloudSail/auto-deploy/issues"
  }
}